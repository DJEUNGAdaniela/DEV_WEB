let student = {

name: "Marie",
age : 20,
courses : []

}

student.age = 21;           // Change l'âge à 21
student.grade = "A";        // Ajoute la propriété grade

student.courses.push("Math", "Physics", "Chemistry");

let physicsIndex = student.courses.indexOf("Physics");


let firstTwoCourses = student.courses.slice(0, 2);

console.log(student);         // Affiche l'objet modifié
console.log("index de physics dans courses est:", physicsIndex);    // Affiche l'index de "Physics"
console.log("les deux premiers cours sont:", firstTwoCourses); // Affiche le nouveau tableau
